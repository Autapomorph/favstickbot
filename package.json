{
  "name": "favstickbot",
  "version": "1.0.0",
  "description": "Telegram bot which helps you mix stickers from multiple packs into a single one",
  "type": "module",
  "keywords": [
    "telegram",
    "bot",
    "stickers"
  ],
  "author": "Autapomorph (https://github.com/Autapomorph)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Autapomorph/favstickbot.git"
  },
  "bugs": {
    "url": "https://github.com/Autapomorph/favstickbot/issues"
  },
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./src/index.js",
    "dev": "cross-env NODE_ENV=development npm run watch",
    "watch": "nodemon src/index.js -e js,mjs,json,yaml",
    "agendash": "node ./src/agendash.js",
    "migrate": "migrate -s ./src/utils/migrations/store",
    "migrate:create": "migrate create -t ./src/utils/migrations/template.js",
    "seed": "node ./seed/index.js",
    "tg-api": "node ./tgApi/index.js",
    "lint": "run-s -c lint:js",
    "lint:js": "eslint **/*.{js,jsx,ts,tsx} --report-unused-disable-directives",
    "lint:staged": "lint-staged --relative",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\" \"test/**/*.{js,jsx,ts,tsx}\"",
    "cm": "cz",
    "release": "standard-version",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@casl/mongoose": "^7.2.2",
    "@faker-js/faker": "^8.4.1",
    "@grammyjs/i18n": "^0.5.1",
    "@grammyjs/ratelimiter": "^1.2.0",
    "@sentry/node": "^7.102.1",
    "agenda": "^5.0.0",
    "agendash": "^4.0.0",
    "bottleneck": "^2.19.5",
    "change-case": "^4.1.2",
    "chrono-node": "^2.7.5",
    "clipboardy": "^2.3.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "fetch-to-curl": "^0.6.0",
    "human-interval": "^2.0.1",
    "migrate": "^2.1.0",
    "mongo-seeding": "^4.0.0",
    "mongodb": "^6.3.0",
    "mongoose": "^8.2.0",
    "mongoose-long": "^0.8.0",
    "open": "^8.4.2",
    "sharp": "^0.33.2",
    "telegraf": "^4.15.3",
    "telegraf-inline-menu": "^6.3.0",
    "telegraf-throttler": "^0.6.0",
    "telegraf-update-logger": "^1.5.0",
    "triple-beam": "^1.4.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-transport": "^4.7.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "ignore": "^5.3.1",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "standard-version": "^9.5.0"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
