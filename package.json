{
  "name": "favstickbot",
  "version": "1.0.0",
  "description": "Telegram bot which helps you mix stickers from multiple packs into a single one",
  "keywords": [
    "telegram",
    "bot",
    "stickers"
  ],
  "author": "Autapomorph (https://github.com/Autapomorph)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Autapomorph/favstickbot.git"
  },
  "bugs": {
    "url": "https://github.com/Autapomorph/favstickbot/issues"
  },
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./src/index.js",
    "dev": "cross-env NODE_ENV=development npm run watch",
    "watch": "nodemon src/index.js -e js,mjs,json,yaml",
    "agendash": "node ./src/agendash.js",
    "migrate": "migrate -s ./src/utils/migrations/store",
    "migrate:create": "migrate create -t ./src/utils/migrations/template.js",
    "lint": "eslint .",
    "lint:staged": "lint-staged --relative",
    "cm": "cz"
  },
  "dependencies": {
    "agenda": "^4.0.1",
    "agendash": "^2.0.0",
    "bottleneck": "^2.19.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "human-interval": "^2.0.1",
    "migrate": "^1.7.0",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.15",
    "node-fetch": "^2.6.1",
    "open": "^7.4.0",
    "sharp": "^0.27.1",
    "telegraf": "^4.0.2",
    "telegraf-i18n": "^6.6.0",
    "telegraf-inline-menu": "^6.1.0",
    "telegraf-throttler": "^0.5.0",
    "telegraf-update-logger": "^1.3.0",
    "winston": "^3.3.3",
    "winston-transport-sentry-node": "^0.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "ignore": "^5.1.8",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint:staged"
    }
  }
}
