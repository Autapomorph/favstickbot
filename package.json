{
  "name": "favstickbot",
  "version": "1.0.0",
  "description": "Telegram bot which helps you mix stickers from multiple packs into a single one",
  "keywords": [
    "telegram",
    "bot",
    "stickers"
  ],
  "author": "Autapomorph (https://github.com/Autapomorph)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Autapomorph/favstickbot.git"
  },
  "bugs": {
    "url": "https://github.com/Autapomorph/favstickbot/issues"
  },
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./src/index.js",
    "dev": "cross-env NODE_ENV=development npm run watch",
    "watch": "nodemon src/index.js -e js,mjs,json,yaml",
    "agendash": "node ./src/agendash.js",
    "migrate": "migrate -s ./src/utils/migrations/store",
    "migrate:create": "migrate create -t ./src/utils/migrations/template.js",
    "seed": "node ./seed/index.js",
    "tg-api": "node ./tgApi/index.js",
    "lint": "eslint .",
    "lint:staged": "lint-staged --relative",
    "cm": "cz",
    "release": "standard-version",
    "prepare": "is-ci || is-docker || husky install"
  },
  "dependencies": {
    "@casl/ability": "^5.4.3",
    "@casl/mongoose": "^6.0.0",
    "@faker-js/faker": "^5.5.3",
    "@grammyjs/i18n": "^0.5.0",
    "@grammyjs/ratelimiter": "^1.1.4",
    "@sentry/node": "^6.16.1",
    "agenda": "^4.2.1",
    "agendash": "^2.1.1",
    "bottleneck": "^2.19.5",
    "change-case": "^4.1.2",
    "chrono-node": "^2.3.5",
    "clipboardy": "^2.3.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "fetch-to-curl": "^0.5.2",
    "human-interval": "^2.0.1",
    "is-ci": "^3.0.1",
    "is-docker": "^2.2.1",
    "migrate": "^1.7.0",
    "mongo-seeding": "^3.7.1",
    "mongodb": "^4.3.0",
    "mongoose": "^6.1.6",
    "mongoose-long": "^0.5.1",
    "node-fetch": "^2.6.6",
    "open": "^8.4.0",
    "sharp": "^0.29.3",
    "telegraf": "^4.6.0",
    "telegraf-inline-menu": "^6.3.0",
    "telegraf-throttler": "^0.6.0",
    "telegraf-update-logger": "^1.5.0",
    "triple-beam": "^1.3.0",
    "winston": "^3.4.0",
    "winston-daily-rotate-file": "^4.5.5",
    "winston-transport": "^4.4.2",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "ignore": "^5.2.0",
    "lint-staged": "^12.1.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
