{
  "name": "favstickbot",
  "version": "1.0.0",
  "description": "Telegram bot which helps you mix stickers from multiple packs into a single one",
  "keywords": [
    "telegram",
    "bot",
    "stickers"
  ],
  "author": "Autapomorph (https://github.com/Autapomorph)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Autapomorph/favstickbot.git"
  },
  "bugs": {
    "url": "https://github.com/Autapomorph/favstickbot/issues"
  },
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./src/index.js",
    "dev": "cross-env NODE_ENV=development npm run watch",
    "watch": "nodemon src/index.js -e js,mjs,json,yaml",
    "agendash": "node ./src/agendash.js",
    "migrate": "migrate -s ./src/utils/migrations/store",
    "migrate:create": "migrate create -t ./src/utils/migrations/template.js",
    "seed": "node ./seed/index.js",
    "tg-api": "node ./tgApi/index.js",
    "lint": "eslint .",
    "lint:staged": "lint-staged --relative",
    "cm": "cz",
    "release": "standard-version",
    "prepare": "is-ci || is-docker || husky install"
  },
  "dependencies": {
    "@casl/ability": "^5.4.4",
    "@casl/mongoose": "^6.0.0",
    "@faker-js/faker": "^7.1.0",
    "@grammyjs/i18n": "^0.5.1",
    "@grammyjs/ratelimiter": "^1.1.5",
    "@sentry/node": "^7.6.0",
    "agenda": "^4.3.0",
    "agendash": "^3.1.0",
    "bottleneck": "^2.19.5",
    "change-case": "^4.1.2",
    "chrono-node": "^2.3.8",
    "clipboardy": "^2.3.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "fetch-to-curl": "^0.5.2",
    "human-interval": "^2.0.1",
    "is-ci": "^3.0.1",
    "is-docker": "^2.2.1",
    "migrate": "^1.8.0",
    "mongo-seeding": "^3.7.2",
    "mongodb": "^4.6.0",
    "mongoose": "^6.3.5",
    "mongoose-long": "^0.5.1",
    "node-fetch": "^2.6.7",
    "open": "^8.4.0",
    "sharp": "^0.30.6",
    "telegraf": "^4.8.3",
    "telegraf-inline-menu": "^6.3.0",
    "telegraf-throttler": "^0.6.0",
    "telegraf-update-logger": "^1.5.0",
    "triple-beam": "^1.3.0",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.7.1",
    "winston-transport": "^4.5.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "ignore": "^5.2.0",
    "lint-staged": "^13.0.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0"
  },
  "standard-version": {
    "skip": {
      "changelog": true
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
