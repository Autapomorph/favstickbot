{
  "name": "favstickbot",
  "version": "1.0.0",
  "description": "Telegram bot which helps you mix stickers from multiple packs into a single one",
  "keywords": [
    "telegram",
    "bot",
    "stickers"
  ],
  "author": "Autapomorph (https://github.com/Autapomorph)",
  "repository": {
    "type": "git",
    "url": "https://github.com/Autapomorph/favstickbot.git"
  },
  "bugs": {
    "url": "https://github.com/Autapomorph/favstickbot/issues"
  },
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run watch",
    "serve": "cross-env NODE_ENV=production npm run watch",
    "watch": "nodemon src/index.js -e js,mjs,json,yaml",
    "migrate": "migrate -s ./src/utils/migrations/store",
    "migrate:create": "migrate create -t ./src/utils/migrations/template.js",
    "lint": "eslint .",
    "lint:staged": "lint-staged --relative",
    "cm": "cz"
  },
  "dependencies": {
    "bottleneck": "^2.19.5",
    "dotenv": "^8.2.0",
    "migrate": "^1.7.0",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.12",
    "nanoid": "^3.1.16",
    "node-fetch": "^2.6.1",
    "sharp": "^0.26.2",
    "telegraf": "^3.38.0",
    "telegraf-i18n": "^6.6.0",
    "telegraf-throttler": "^0.2.0",
    "telegraf-update-logger": "^1.2.0",
    "winston": "^3.3.3",
    "winston-transport-sentry-node": "^0.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "ignore": "^5.1.8",
    "lint-staged": "^10.5.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint:staged"
    }
  }
}
